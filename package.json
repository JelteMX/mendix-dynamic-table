{
  "name": "dynamictable",
  "widgetName": "DynamicTable",
  "version": "1.2.0",
  "description": "Dynamic (Tree) Table for Mendix",
  "copyright": "Jelte Lagendijk 2019",
  "author": "Jelte Lagendijk <jelte.lagendijk@mendix.com>",
  "config": {
    "widgetPath": "./dist/MxTestProject/widgets",
    "projectPath": "./dist/MxTestProject/",
    "mendixHost": "http://windows:8080",
    "developmentPort": "3000"
  },
  "packagePath": "com.mendix.widget.custom",
  "scripts": {
    "start": "node --max_old_space_size=4112 ./node_modules/.bin/concurrently \"pluggable-widgets-tools start:server --open\" \"npm:dev\"",
    "dev": "node --max_old_space_size=4112 ./node_modules/.bin/pluggable-widgets-tools start:js",
    "build": "node --max_old_space_size=4112 ./node_modules/.bin/pluggable-widgets-tools build:js",
    "lint": "node --max_old_space_size=4112 ./node_modules/.bin/pluggable-widgets-tools lint",
    "lint:fix": "node --max_old_space_size=4112 ./node_modules/.bin/pluggable-widgets-tools lint:fix",
    "pretest": "npm run lint && npm run build",
    "test-old": "pluggable-widgets-tools test:unit --no-cache --ci && npm run test:e2e",
    "test": "npm run test:unit --no-cache --ci && npm run test:e2e",
    "test:unit": "node --max_old_space_size=4112 ./node_modules/.bin/pluggable-widgets-tools test:unit --coverage",
    "test:travis": "jest --projects ./jest.config.js && cat ./dist/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./dist/coverage",
    "test:e2e": "export URL=https://dynamictabletest-sandbox.mxapps.io/ || set URL=https://dynamictabletest-sandbox.mxapps.io/ && node --max_old_space_size=4112 ./node_modules/.bin/pluggable-widgets-tools test:e2e:ts",
    "test:e2e:dev": "export DEBUG=true || set DEBUG=true && node --max_old_space_size=4112 ./node_modules/.bin/pluggable-widgets-tools test:e2e:ts",
    "test:e2e:mac": "export URL=http://windows:8080/ && export DEBUG=true || set DEBUG=true && pluggable-widgets-tools test:e2e:ts",
    "prerelease": "npm run lint",
    "release": "node --max_old_space_size=4112 ./node_modules/.bin/pluggable-widgets-tools release:js",
    "analyze": "node --max_old_space_size=4112 ./node_modules/.bin/pluggable-widgets-tools release:js --analyze"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@mendix/pluggable-widgets-tools": "git@github.com:mendix/pluggable-widgets-tools.git",
    "@types/big.js": "^4.0.5",
    "@types/classnames": "^2.2.9",
    "@types/dompurify": "^2.0.1",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jasmine": "^3.4.0",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.147",
    "@types/mustache": "^0.8.32",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-resize-detector": "^4.2.0",
    "@types/react-test-renderer": "^16.8.1",
    "@types/uuid": "^3.4.6",
    "@types/webdriverio": "^4.13.0",
    "mendix-client": "^7.15.8",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.6",
    "@jeltemx/mendix-react-widget-utils": "^0.3.6",
    "antd": "^3.25.1",
    "array-to-tree": "^3.3.0",
    "big.js": "^5.2.2",
    "classnames": "^2.2.6",
    "core-js": "^3.4.1",
    "dompurify": "^2.0.7",
    "lodash": "^4.17.15",
    "mobx": "^4.13.0",
    "mobx-react": "^6.1.3",
    "mustache": "^3.1.0",
    "react-resize-detector": "^4.2.1",
    "uuid": "^3.4.0"
  }
}
